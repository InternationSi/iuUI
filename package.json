{
  "name": "vultures-vue",
  "private": false,
  "version": "0.0.2",
  "keywords": [
    "vultures",
    "vue",
    "vultures-vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "lint": "eslint . --fix --ext .ts,.vue,.js,.tsx ",
    "prettier": "prettier --write ."
  },
  "files": [
    "lib"
  ],
  "main": "lib/vultures-vue.umd.js",
  "module": "lib/vultures-vue.es.js",
  "exports": {
    "./lib/style.css": "./lib/style.css",
    ".": {
      "import": "./lib/vultures-vue.es.js",
      "require": "./lib/vultures-vue.umd.js"
    }
  },
  "dependencies": {
    "echarts": "^5.3.3",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "eslint": "^8.8.0",
    "eslint-plugin-vue": "^8.4.0",
    "sass": "^1.49.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitepress": "^0.21.6",
    "vitepress-theme-demoblock": "^1.4.2",
    "vue-tsc": "^0.29.8"
  }
}
